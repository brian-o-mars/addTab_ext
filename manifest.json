{
    "manifest_version": 3,
    "name": "Add New Tab To Current Group",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "icons": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png",
        "48": "images/favicon-48x48.png",
        "128": "images/favicon-128x128.png"
      },
    "action": {
        "default_popup": "Popup.html",
        "default_title": "AddTab"
    },
    "permissions": [
        "tabs"
      ]
}